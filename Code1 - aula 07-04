#include <stdio.h>
#include <conio.h>


int main()
{
    float h, r, pi = 3.14;
    float vol;
    
    printf("Digite o valor do raio: ");
    scanf("%f", &r);

    printf("Digite o valor da altura: ");
    scanf("%f", &h);
    
    vol = h * r * pi;
    
    printf("O Volume Ã©: %f \n \n",vol);

    return 0;
}
